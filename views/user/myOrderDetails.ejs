    <script>

async function generateAndDownloadInvoice(orderId,index) {
  let timerInterval;

  // Display a SweetAlert2 modal
  Swal.fire({
    title: 'Download Started!',
    html: 'I will close in <b></b> milliseconds.',
    timer: 4000,
    timerProgressBar: true,
    didOpen: () => {
      Swal.showLoading();
      const b = Swal.getHtmlContainer().querySelector('b');
      timerInterval = setInterval(() => {
        b.textContent = Swal.getTimerLeft();
      }, 100);
    },
    willClose: () => {
      clearInterval(timerInterval);
    }
  }).then((result) => {
    if (result.dismiss === Swal.DismissReason.timer) {
      console.log('I was closed by the timer');
    }
  });

  try {
    // Send a POST request to the server to initiate the download
    const response = await fetch(`/generateinvoice/${orderId}/${index}`, {
      method: 'get',
      headers: {
        'Content-Type': 'application/json',
      },
      
    });

    if (response.ok) {
      
      window.location.href = `/downloadinvoice/${orderId}`;
    }
  } catch (error) {
    console.error('Error in downloading the invoice:', error);
  }


}



