    width: 70px;
    font-size: 25px;
    font-weight: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
}
.search-btn:hover{
transition: 0.2s ease-in-out;
font-size: 30px;
font-weight: 800;
cursor: pointer;
}


