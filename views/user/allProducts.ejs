}





.search{
    box-shadow:0px 0px 20px 1px #ffffff ;
    border: 0;
    border-radius: 30px 0 0 30px;
    padding: 0 10px 0 10px;
    color: black;
    height: 40px;
    width: 400px;
    font-size: 25px;
    font-weight: 1000;
    background-color: rgb(234, 234, 234);
}
.search::selection{
background-color: red;
color: white;
}
.search:focus{
border: 0;
outline: 0;
}
.search:hover{
cursor: text;
}





.search-btn{
    transition: 0.2s ease-in-out;
